<?php

  $listaRetificacoesAvFisioterapeutica = new Conn(); //instância do banco
  $listaRetificacoesAvFisioterapeutica->setSQL("select CONVERT(char(16),datacadastro, 103) as datacadastro,CONVERT(char(5),datacadastro, 108) as horacadastro,ficha_av_fisioterapeutica_id,id   from  formularios.dbo.tb_ficha_avaliacao_fisioterapeutica where internacao_id = ".$codInternacao."  order by id desc");

while ($valorRetificacoes = mssql_fetch_array($listaRetificacoesAvFisioterapeutica->getObjQuery())):
?>
