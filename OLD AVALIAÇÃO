function AbreItens(div_do_item){
var sn = document.getElementById('div_item').value;
		
	if(sn == ''){
		document.getElementById('div_item').value = div_do_item;
		document.getElementById(div_do_item).style.display = "block";
	} else {
		if (sn == div_do_item){
			document.getElementById(div_do_item).style.display = "none";
			document.getElementById('div_item').value = '';   
			return false;
		}            
		document.getElementById(sn).style.display = "none";
		document.getElementById(div_do_item).style.display = "block";
		document.getElementById('div_item').value = div_do_item;
	}
}

function mudaDisplay(nome_div,valor){

	if (valor!= '0'){
		if (document.getElementById(nome_div).style.display=='block'){
			document.getElementById(nome_div).style.display = "none";
		
		}else {
		
			document.getElementById(nome_div).style.display = "block";
		}
	}else{
		document.getElementById(nome_div).style.display = "none";
	}
}


function Trim(str){return str.replace(/^\s+|\s+$/g,"");}

function validaForm(situacao){

	var frc_cardiaca  =  document.getElementById('frc_cardiaca').value;
	var respiracao  =  document.getElementById('respiracao').value;
	var pa  =  document.getElementById('pa').value;
	var SaO2AA  =  document.getElementById('SaO2AA').value;
	var SaO202  =  document.getElementById('SaO202').value;
	
	
	if (Trim(frc_cardiaca) == '') {
		AlertErro('frc_cardiaca');HabilitaFocus('frc_cardiaca','item2'); return false;}

	if (Trim(respiracao) == '') {
		AlertErro('respiracao');HabilitaFocus('respiracao','item2'); return false;}
		
	if (Trim(pa) == '') {
		AlertErro('pa');HabilitaFocus('pa','item2'); return false;}
	
	if (Trim(SaO2AA) == '') {
		AlertErro('SaO2AA');HabilitaFocus('SaO2AA','item2'); return false;}
	
	if (Trim(SaO202) == '') {
		AlertErro('SaO202');HabilitaFocus('SaO202','item2'); return false;}
		
	if (document.getElementById('dispositivo_auxiliar_MB1').checked) {
		if (document.getElementById('qtd_muleta').value == '0'){
			AlertErro('com a quantidade de muletas');HabilitaFocus('qtd_muleta','item1'); return false;}	
	}
	
	if (document.getElementById('dispositivo_auxiliar_MB2').checked) {
		if (document.getElementById('qtd_bengala').value == '0'){
			AlertErro('com a quantidade de bengalas');HabilitaFocus('qtd_bengala','item1'); return false;}	
	}
	
	
	if (Trim(situacao) == 'incluido'){
		document.getElementById('finalizado').value = 'N';
		formAvFisioterapeutica.submit();
	}else{
		document.getElementById('finalizado').value = 'S';
		formAvFisioterapeutica.submit();
	}
	


	
}

function AlertErro(campo){
	alert ('Favor preencher o campo '+campo+'.');
}

function HabilitaFocus(campo,div){

		if(div=='item2'){
		  document.getElementById('item2').style.display="block";document.getElementById('item1').style.display="none";  document.getElementById('item3').style.display="none"; document.getElementById('item4').style.display="none"; document.getElementById('item5').style.display="none"; document.getElementById('item6').style.display="none";document.getElementById('item7').style.display="none";document.getElementById(campo).focus(); 
		}
		
}


function checar(){
	if (document.getElementById('musculo_outro').checked){
		 document.getElementById('especifique_musculo').style.display="block";
	}else{
		document.getElementById('especifique_musculo').style.display="none";
		document.getElementById('especifique_musculo_outro').value="";
	}
	
	
	
	if (document.getElementById('necessita_avaliacao_S').checked) {
		 document.getElementById('avg_neurologica').style.display="block";
	}else{
		document.getElementById('avg_neurologica').style.display="none";
		
	}
	
	if (document.getElementById('dispositivo_auxiliar_MB1').checked) {
		 document.getElementById('qtd_muleta').style.display="";
	}else{
		document.getElementById('qtd_muleta').style.display="none";
		document.getElementById('qtd_bengala').value = '0';
		
	}
	
	if (document.getElementById('dispositivo_auxiliar_MB2').checked) {
		 document.getElementById('qtd_bengala').style.display="";
	}else{
		document.getElementById('qtd_bengala').style.display="none";
		document.getElementById('qtd_muleta').value = '0';
		
	}
	
	
}

9
	
	
	
	
	
	
	


}

